[Unit]
Description=Soft IOC for BO dipoles
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-dipoles'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-dipoles
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-Fam:MA-B
