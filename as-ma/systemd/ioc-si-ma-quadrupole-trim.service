[Unit]
Description=Soft IOC for SI dipole magnet families
After=multi-user.target

[Service]
Group=fac
User=fac
Type=simple
WorkingDirectory=/home/fac/sirius-iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-si-ma-quadrupole-trim'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-si-ma-quadrupole-trim
ExecStart=/usr/local/bin/sirius-ioc-si-ma-quadrupole-trim.py
