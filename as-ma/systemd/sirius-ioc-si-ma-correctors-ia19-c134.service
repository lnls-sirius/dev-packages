[Unit]
Description=Power Supply IOC - SI Correctors Subsections 19C1, 19C3, 19C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia19-c134'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia19-c134
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-19C1:MA-CH \
SI-19C1:MA-CV \
SI-19C4:MA-CH \
SI-19C4:MA-CV \
SI-19C3:MA-CH \
SI-19C3:MA-CV-1 \
SI-19C3:MA-CV-2
