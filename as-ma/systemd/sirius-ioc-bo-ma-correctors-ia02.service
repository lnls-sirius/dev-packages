[Unit]
Description=Soft IOC for BO corrector magnets ia02
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-tb-ma-correctors-ia02'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-tb-ma-correctors-ia02
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-03U:MA-CH \
BO-03U:MA-CV \
BO-05U:MA-CH \
BO-05U:MA-CV
