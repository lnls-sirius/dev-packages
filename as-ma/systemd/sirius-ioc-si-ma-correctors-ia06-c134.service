[Unit]
Description=Soft IOC for SI Correctors Subsections 06C1, 06C3, 06C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia06-c134'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia06-c134
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-06C1:MA-CH \
SI-06C1:MA-CV \
SI-06C4:MA-CH \
SI-06C4:MA-CV \
SI-06C3:MA-CH \
SI-06C3:MA-CV-1 \
SI-06C3:MA-CV-2
