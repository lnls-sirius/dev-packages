[Unit]
Description=Soft IOC for SI Trims and Skew Quadrupoles Subsections 13M1, 13M2
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-trims-qs-m12-ia13'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-trims-qs-m12-ia13
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-13M2:MA-QFA \
SI-13M2:MA-QDA \
SI-14M1:MA-QFB \
SI-14M1:MA-QDB1 \
SI-14M1:MA-QDB2 \
SI-13M1:MA-QS \
SI-13M2:MA-QS
