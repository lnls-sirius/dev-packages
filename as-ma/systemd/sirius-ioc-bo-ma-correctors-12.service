[Unit]
Description=Soft IOC for BO corrector magnets 12
After=multi-user.target

[Service]
Group=fac
User=fac
Type=simple
WorkingDirectory=/home/fac/sirius-iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-12'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-12
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-43U:MA-CH \
BO-43U:MA-CV \
BO-45U:MA-CH \
BO-45U:MA-CV
