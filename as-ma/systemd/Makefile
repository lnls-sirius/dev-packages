	@printf "Installing sirius-ioc-ts-pm services...\n"
	cp -rf sirius-ioc-ts-pm-ejeseptf.service /etc/systemd/system/
	cp -rf sirius-ioc-ts-pm-ejeseptg.service /etc/systemd/system/
