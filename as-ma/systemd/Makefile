	systemctl start sirius-ioc-ts-pm-ejeseptg.service
	systemctl start sirius-ioc-ts-pm-injseptf.service
	systemctl start sirius-ioc-ts-pm-injseptg1.service
	systemctl start sirius-ioc-ts-pm-injseptg2.service

disable-ts-ma:
	@printf "Disabling sirius-ioc-ts-ma services...\n"
	systemctl disable sirius-ioc-ts-ma-correctors.service
	systemctl disable sirius-ioc-ts-ma-quadrupoles-34.service
	systemctl disable sirius-ioc-ts-ma-quadrupoles-12.service
	systemctl disable sirius-ioc-ts-ma-dipoles.service

disable-ts-pm:
	@printf "Disabling sirius-ioc-ts-pm services...\n"
	systemctl disable sirius-ioc-ts-pm-ejeseptf.service
	systemctl disable sirius-ioc-ts-pm-ejeseptg.service
	systemctl disable sirius-ioc-ts-pm-injseptf.service
	systemctl disable sirius-ioc-ts-pm-injseptg1.service
