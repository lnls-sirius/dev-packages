[Unit]
Description=Soft IOC for SI Trims and Skew Quadrupoles Subsections 14C1, 14C2, 14C3, 14C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-trims-qs-c1234-ia14'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-trims-qs-c1234-ia14
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-14C1:MA-Q1 \
SI-14C1:MA-Q2 \
SI-14C2:MA-Q3 \
SI-14C2:MA-Q4 \
SI-14C4:MA-Q1 \
SI-14C4:MA-Q2 \
SI-14C3:MA-Q3 \
SI-14C3:MA-Q4 \
SI-14C1:MA-QS \
SI-14C2:MA-QS \
SI-14C3:MA-QS
