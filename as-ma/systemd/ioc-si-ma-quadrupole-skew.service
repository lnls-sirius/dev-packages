[Unit]
Description=Soft IOC for SI Quadrupole Skew
After=multi-user.target

[Service]
Group=fac
User=fac
Type=simple
WorkingDirectory=/home/fac/sirius-iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-si-ma-quadrupole-skew'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-si-ma-quadrupole-skew
ExecStart=/usr/local/bin/sirius-ioc-si-ma-quadrupole-skew.py
