[Unit]
Description=Soft IOC for SI Trims and Skew Quadrupoles Subsections 08C1, 08C2, 08C3, 08C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-trims-qs-c1234-ia08'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-trims-qs-c1234-ia08
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-08C1:MA-Q1 \
SI-08C1:MA-Q2 \
SI-08C2:MA-Q3 \
SI-08C2:MA-Q4 \
SI-08C4:MA-Q1 \
SI-08C4:MA-Q2 \
SI-08C3:MA-Q3 \
SI-08C3:MA-Q4 \
SI-08C1:MA-QS \
SI-08C2:MA-QS \
SI-08C3:MA-QS
