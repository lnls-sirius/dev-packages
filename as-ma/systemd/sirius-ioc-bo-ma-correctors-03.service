[Unit]
Description=Soft IOC for BO corrector magnets 03
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-03'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-03
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-07U:MA-CH \
BO-07U:MA-CV \
BO-09U:MA-CH \
BO-09U:MA-CV
