[Unit]
Description=Power Supply IOC - SI Correctors Subsections 15C1, 15C3, 15C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia15-c134'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia15-c134
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-15C1:MA-CH \
SI-15C1:MA-CV \
SI-15C4:MA-CH \
SI-15C4:MA-CV \
SI-15C3:MA-CH \
SI-15C3:MA-CV-1 \
SI-15C3:MA-CV-2
