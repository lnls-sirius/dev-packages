[Unit]
Description=Soft IOC for BO corrector magnets 01
After=multi-user.target

[Service]
Group=fac
User=fac
Type=simple
WorkingDirectory=/home/fac/sirius-iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-01'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-01
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-01U:MA-CH \
BO-01U:MA-CV \
BO-02D:MA-QS
