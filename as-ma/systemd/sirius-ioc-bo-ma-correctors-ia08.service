[Unit]
Description=Soft IOC for BO corrector magnets ia08
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-ia08'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-ia08
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-19U:MA-CH \
BO-19U:MA-CV \
BO-21U:MA-CH \
BO-21U:MA-CV
