[Unit]
Description=Soft IOC for BO corrector magnets ia17
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-tb-ma-correctors-ia17'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-tb-ma-correctors-ia17
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-43U:MA-CH \
BO-43U:MA-CV \
BO-45U:MA-CH \
BO-45U:MA-CV
