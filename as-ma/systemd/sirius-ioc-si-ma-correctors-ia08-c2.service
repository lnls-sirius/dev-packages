[Unit]
Description=Power Supply IOC - SI Correctors Subsections 08C2
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia08-c2'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia08-c2
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-08C2:MA-CH \
SI-08C2:MA-CV-1 \
SI-08C2:MA-CV-2
