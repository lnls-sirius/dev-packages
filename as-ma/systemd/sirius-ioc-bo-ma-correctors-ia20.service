[Unit]
Description=Soft IOC for BO corrector magnets ia20
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-ia20'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-ia20
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-47U:MA-CH \
BO-47U:MA-CV \
BO-49D:MA-CH \
BO-49U:MA-CV
