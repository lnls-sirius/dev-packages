[Unit]
Description=Soft IOC for BO corrector magnets 07
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-07'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-07
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-23U:MA-CH \
BO-23U:MA-CV \
BO-25U:MA-CH \
BO-25U:MA-CV
