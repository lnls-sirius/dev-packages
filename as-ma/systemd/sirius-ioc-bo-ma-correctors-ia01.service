[Unit]
Description=Soft IOC for BO corrector magnets ia01
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-tb-ma-correctors-ia01'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-tb-ma-correctors-ia01
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-01U:MA-CH \
BO-01U:MA-CV \
BO-02D:MA-QS
