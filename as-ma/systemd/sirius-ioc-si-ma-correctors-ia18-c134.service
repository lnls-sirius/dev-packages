[Unit]
Description=Soft IOC for SI Correctors Subsections 18C1, 18C3, 18C4
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs-log
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia18-c134'
EnvironmentFile=-/home/sirius/iocs-log/env-sirius-ioc-si-ma-correctors-ia18-c134
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
SI-18C1:MA-CH \
SI-18C1:MA-CV \
SI-18C4:MA-CH \
SI-18C4:MA-CV \
SI-18C3:MA-CH \
SI-18C3:MA-CV-1 \
SI-18C3:MA-CV-2
