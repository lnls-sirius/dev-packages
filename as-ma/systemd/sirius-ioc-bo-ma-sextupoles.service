[Unit]
Description=Soft IOC for BO sextupoles
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-sextupoles'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-sextupoles
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-Fam:MA-SF \
BO-Fam:MA-SD
