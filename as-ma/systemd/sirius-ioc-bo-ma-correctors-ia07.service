[Unit]
Description=Soft IOC for BO corrector magnets ia07
After=multi-user.target

[Service]
Group=sirius
User=sirius
Type=simple
WorkingDirectory=/home/sirius/iocs
StandardOutput=syslog
StandardError=syslog
ExecStartPre=/bin/bash --login -c 'env > /tmp/bashrc-sirius-ioc-tb-ma-correctors-ia07'
EnvironmentFile=-/tmp/bashrc-sirius-ioc-tb-ma-correctors-ia07
ExecStart=/usr/local/bin/sirius-ioc-as-ma.py \
BO-15U:MA-CH \
BO-15U:MA-CV \
BO-17U:MA-CH \
BO-17U:MA-CV
